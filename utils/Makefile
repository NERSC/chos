#
# Configuration options for building are stored in the Makefile.in file.
# This file is generated by running the ./configure script.
#
# 

CC = gcc

MFLAGS =
KERNEL=`uname -r`
IFLAGS = -I/lib/modules/$(KERNEL)/build/include
CFLAGS = -O3 -DMODULE -DSYSCALL_NOT_EXPORTED $(IFLAGS)
VERSION = 0.02


all:  chos

chos: chos.o

testinstall:
	cp chos /usr/bin
	chmod u+s /usr/bin/chos
clean: 
	rm -f $(OBJS) chos chos.o
